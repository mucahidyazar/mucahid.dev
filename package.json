{
  "name": "mucahid.dev",
  "version": "v2.1.1",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucahidyazar/mucahid.dev.git"
  },
  "author": {
    "name": "Mucahid Yazar",
    "email": "mucahidyazar@gmail.com",
    "url": "https://mucahid.dev"
  },
  "homepage": "https://mucahid.dev",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "dev:up": "docker-compose -f docker-compose.dev.yml up",
    "prod:up": "docker-compose -f docker-compose.prod.yml up",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --ci --collectCoverage",
    "prepare": "husky install",
    "generate:sitemap": "cross-env next-sitemap --config others/next-sitemap.js",
    "generate:rss": "ts-node others/generate-rss.js",
    "commit": "cz",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "typecheck": "tsc -b",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.4",
    "@next/bundle-analyzer": "^12.2.3",
    "@openzeppelin/contracts": "^4.7.2",
    "@prisma/client": "4.1.1",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "framer-motion": "6.5.1",
    "immer": "^9.0.15",
    "next": "^12.2.3",
    "next-auth": "4.10.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.3.0",
    "next-redux-wrapper": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-lottie": "^1.2.3",
    "react-markdown": "^8.0.3",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-responsive": "^9.0.0-beta.10",
    "react-select": "^5.4.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.0.7",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "sharp": "^0.30.7",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "5.3.5",
    "twitter": "^1.7.1",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/js-cookie": "^3.0.2",
    "@types/next-auth": "3.15.0",
    "@types/node": "18.6.3",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-lottie": "^1.2.6",
    "@types/react-modal": "^3.13.1",
    "@types/react-slick": "^0.23.10",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.25",
    "autoprefixer": "^10.4.8",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^10.4.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "feed": "^4.2.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "next-sitemap": "^3.1.15",
    "nodemailer": "^6.7.7",
    "prettier": "2.7.1",
    "prisma": "^4.1.1",
    "ts-jest": "^28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*"
  ]
}
