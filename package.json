    "@next/font": "^13.0.5",
    "@tanstack/react-query": "^4.18.0",
    "@tanstack/react-table": "^8.7.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
