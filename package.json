  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucahidyazar/mucahid.dev.git"
  },
  "author": {
    "name": "Mucahid Yazar",
    "email": "mucahidyazar@gmail.com",
    "url": "https://mucahid.dev"
